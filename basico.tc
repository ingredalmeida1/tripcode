-> testando comentario <-
EXTERIOR teste 10 

BAGAGEM VOUCHER variavel, nome <-> "leticia";
BAGAGEM VOUCHER julia;
BAGAGEM VOUCHER juliana, maria;

ROTEIRO soma(MILHAS a) >>> MILHAS <<<

ROTEIRO trip() >>> 

    CHECKIN([MILHAS, DOLAR], teste, valor);

    TURISTANDO (FERIAS) >>>
        BAGAGEM DOLAR teste <-> teste + -20.5;
        ALFANDEGA (teste = 20) >>>
            POUSAR;
        <<< 
        teste <-> FERIAS NOT;
    <<<

    ALFANDEGA (20) >>>
        BAGAGEM DOLAR teste <-> teste + -20.5;
    <<< TRIBUTADO >>>
        DECOLAR (ORIGEM 0, DESTINO 10, ESCALA 2) >>>
            BAGAGEM MILHAS valor;
            valor, teste <-> 'juliana';
        <<<
    <<< ISENTO >>>
        EMBARCAR soma (teste + 0);
        teste <-> EMBARCAR soma (teste + 0);
        teste <-> teste + EMBARCAR soma (teste + 0);
    <<<

    CHECKOUT("Resultado final" <> MILHAS valor <> "ok!");

    DESPACHAR 0;

    -> testando outro comentario <-
<<< MILHAS <<<
