-> Repetição incorreta em TripCode <-

ROTEIRO trip() >>>
    BAGAGEM MILHAS soma;
    BAGAGEM MILHAS valor;

    valor <-> 2;

    -> DECOLAR precisa dos três parâmetros <-
    DECOLAR (ORIGEM 0, DESTINO 10) >>> 
        soma <-> soma + valor;
    <<<

    BAGAGEM DOLAR preco <-> 1.0;
    BAGAGEM DOLAR aumento <-> 0.5;

    TURISTANDO (FERIAS) >>>
        ALFANDEGA(preco = 10) >>>
            POUSAR;
        <<<
        preco <-> preco + aumento;
    <<<

    DESPACHAR 0;
<<< MILHAS <<<