-> Condição Correta em TripCode <-

ROTEIRO trip() >>> 
    BAGAGEM MILHAS nota1;
    BAGAGEM MILHAS nota2;

    CHECKIN([MILHAS, MILHAS], nota1, nota2);

    ALFANDEGA (nota1 + nota2 >= 60) >>>
        CHECKOUT("Aprovado!");
    <<< TRIBUTADO (nota1 + nota2 <= 40)>>>
        CHECKOUT("Reprovado!");
    <<< ISENTO >>>
       CHECKOUT("Exame Final!");
    <<<
    
    DESPACHAR 0;
<<< MILHAS <<<